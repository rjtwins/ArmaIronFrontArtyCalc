        velFilled = False
        
    if coordFilled :
        da = altDelta(z1, z2)
        dist = rangeDelta(x1, y1, x2, y2)

        top.orange.delete(0, END)
        top.orange.insert(0, str(int(dist)))
        
    if coordFilled and velFilled :
        goodMath = True
        try:
            elv1, elv2 = elevation(velocity, grav, dist, da)
        except ValueError:
            goodMath = False
            top.orange_7.delete(0, END)
            top.orange_7.insert(0, "n/a")

        if goodMath :
            top.orange_7.delete(0, END)
            top.orange_7.insert(0, str(elv2))

    
