grav = 9.81
import math as math

def rangeDelta(tx, ty, bx, by):
    dx = ty - by
    dy = tx - bx
    return 100*math.sqrt(dx**2 + dy**2)

def altDelta(ta, ba):
    return ta - ba
    
def elevation(velocity, grav, dist, da):
    elv1 = math.degrees(math.atan((velocity**2+math.sqrt(velocity**4-grav*((grav*dist**2)+(2*da*velocity**2))))/(grav*dist)))
    elv2 = math.degrees(math.atan((velocity**2-math.sqrt(velocity**4-grav*((grav*dist**2)+(2*da*velocity**2))))/(grav*dist)))
    return elv1, elv2

